{
  "editor.suggestSelection": "first",
  "git.autofetch": true,
  "[beancount]": {
    "editor.defaultFormatter": "dongfg.vscode-beancount-formatter"
  },
  "beancountFormatter.fixedCJKWidth": true,
  "editor.renderControlCharacters": true,
  "emmet.triggerExpansionOnTab": true,
  "indentRainbow.includedLanguages": [],
  "editor.fontFamily": "CaskaydiaCove Nerd Font Mono, JetBrainsMono Nerd Font Mono, FiraCode Nerd Font Mono, Hack Nerd Font Mono, SauceCodePro Nerd Font Mono",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "cSpell.enableFiletypes": [
    "lookml",
    "powershell",
    "python",
    "markdown"
  ],
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "prettier.useEditorConfig": true,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "switch:command",
    "stash-push:command"
  ],
  "omnisharp.enableRoslynAnalyzers": true,
  "[powershell]": {
    "editor.defaultFormatter": "ms-vscode.powershell",
    "editor.tabSize": 2
  },
  "[xml]": {
    "editor.defaultFormatter": "redhat.vscode-xml"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.renderWhitespace": "all",
  "indentRainbow.colorOnWhiteSpaceOnly": true,
  "editor.guides.indentation": false,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "editor.wordWrap": "off",
  "editor.rulers": [
    120
  ],
  "diffEditor.wordWrap": "off",
  "editor.inlayHints.enabled": "on",
  "redhat.telemetry.enabled": false,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "bracketPairColorizer.depreciation-notice": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.autopep8"
  },
  "[yaml]": {
    "editor.autoIndent": "advanced",
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    // "editor.defaultFormatter": "kennylong.kubernetes-yaml-formatter",
    // "editor.defaultFormatter": "esbenp.prettier-vscode"
    "editor.formatOnSave": false
  },
  "kubernetes-yaml-formatter.compactSequenceIndent": true,
  "kubernetes-yaml-formatter.includeDocumentStart": true,
  "blockman.n01LineHeight": 0,
  "editor.guides.bracketPairs": true,
  "sql.linter.run": "onType",
  "extensions.ignoreRecommendations": true,
  "gitlens.showWelcomeOnInstall": false,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.defaultProfile.windows": "PowerShell Core",
  "terminal.integrated.profiles.windows": {
    "PowerShell Core": {
      "path": "pwsh.exe",
      "icon": "terminal-powershell",
      "args": [
        "-NoLogo"
      ]
    },
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell",
      "args": [
        "-NoLogo"
      ]
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [
        "/k"
      ],
      "icon": "terminal-cmd"
    }
  },
  "[properties]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  // Disable welcome screen on startup
  "workbench.welcome.enabled": false,
  // Insert spaces when pressing Tab
  "editor.insertSpaces": true,
  // Minimap
  "editor.minimap.enabled": true,
  // A multiplier to be used on the `deltaX` and `deltaY` of mouse wheel scroll events
  "editor.mouseWheelScrollSensitivity": 5,
  // Extensions to be installed by the Extension Manager
  "extension-manager.extensions": [
    "eg2.tslint", // TypeScript linter
    "ms-vscode.cpptools", // Cpp tools
    "msjsdiag.debugger-for-chrome", // Debugger for Chrome
    "robertohuertasm.vscode-icons" // Cool icons for files
  ],
  // When enabled, will trim trailing whitespace when you save a file
  "files.trimTrailingWhitespace": true,
  // Default end of line character
  "files.eol": "\n",
  // Glob patterns for excluding folders and files
  "files.exclude": {
    "**/*.build": true,
    "**/*.git": true,
    "**/*.err": true,
    "**/*.rec": true,
    "**/*.log": false,
    "**/*.evt": true,
    "**/buildfre.*": true,
    "**/buildchk.*": true,
    "**/.DS_Store": true
  },
  // Glob patterns for excluding files and folders in searches
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    ".build/**": true,
    "out*/**": true,
    "i18n/**": true,
    "extensions/**/out/**": true,
    "*.min.js": true
  },
  // Enable linter for TypeScript files
  "tslint.enable": true,
  "files.associations": {
    "deque": "cpp",
    "list": "cpp",
    "vector": "cpp",
    "xstring": "cpp"
  },
  "git.ignoreMissingGitWarning": true,
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "workbench.iconTheme": "material-icon-theme",
  "grammarly.config.documentDialect": "american",
  "terminal.integrated.fontFamily": "FiraCode Nerd Font Mono, monospace",
  "prettier.withNodeModules": true,
  "trailing-spaces.trimOnSave": true,
  "prettier.enable": true,
  "includeDocumentStart": true,
  "update.enableWindowsBackgroundUpdates": false,
  "extensions.autoCheckUpdates": false,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[typescriptreact]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features"
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    // "editor.defaultFormatter": "vscode.typescript-language-features"
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Visual Studio Dark",
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": false
  },
  "task.allowAutomaticTasks": "on",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "editor.accessibilitySupport": "off",
  "editor.largeFileOptimizations": false,
  "security.workspace.trust.untrustedFiles": "open",
  "githubPullRequests.pullBranch": "never",
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "explorer.confirmPasteNative": false
}
