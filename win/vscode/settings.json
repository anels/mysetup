{
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.autofetch": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[beancount]": {
    "editor.defaultFormatter": "dongfg.vscode-beancount-formatter"
  },
  "beancountFormatter.fixedCJKWidth": true,
  "sync.autoDownload": true,
  "sync.autoUpload": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.renderControlCharacters": true,
  "emmet.triggerExpansionOnTab": true,
  "indentRainbow.includedLanguages": [],
  "workbench.iconTheme": "vscode-great-icons",
  "editor.fontFamily": "JetBrainsMono NFM, FuraMono NF, SauceCodePro NFM, UbuntuMono NF, JetBrains Mono NL",
  "editor.fontLigatures": true,
  "editor.fontSize": 17,
  "cSpell.enableFiletypes": ["lookml", "powershell", "python"],
  "git.confirmSync": false,
  "git.enableSmartCommit": true,
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.useEditorConfig": false,
  "gitlens.gitCommands.skipConfirmations": [
    "fetch:command",
    "switch:command",
    "stash-push:command"
  ],
  "omnisharp.enableRoslynAnalyzers": true,
  "editor.defaultFormatter": "EditorConfig.EditorConfig",
  "[powershell]": {
    "editor.defaultFormatter": "ms-vscode.powershell",
    "editor.tabSize": 2
  },
  "[xml]": {
    "editor.defaultFormatter": "DotJoshJohnson.xml"
  },
  "[shellscript]": {
    "editor.defaultFormatter": "foxundermoon.shell-format"
  },
  "editor.renderWhitespace": "all",
  "indentRainbow.colorOnWhiteSpaceOnly": true,
  "editor.guides.indentation": false,
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "editor.wordWrap": "off",
  "editor.rulers": [80],
  "diffEditor.wordWrap": "off",
  "editor.inlayHints.enabled": false,
  "redhat.telemetry.enabled": false,
  "workbench.colorTheme": "Visual Studio 2019 Dark",
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "cSpell.userWords": [
    "cursum",
    "Kubernetes",
    "msbuild",
    "Xunit"
  ],
  "bracketPairColorizer.depreciation-notice": false,
  "[python]": {
    "editor.defaultFormatter": "ms-python.python"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[latex]": {
    "editor.defaultFormatter": "mathematic.vscode-latex"
  },
  "blockman.n01LineHeight": 0,
  "editor.guides.bracketPairs": false,
  "tabnine.experimentalAutoImports": true,
  "[sql]": {
    "editor.defaultFormatter": "dorzey.vscode-sqlfluff"
  },
  "sql.linter.run": "onType",
  "diffEditor.ignoreTrimWhitespace": false,
  "extensions.ignoreRecommendations": true,
  "gitlens.showWelcomeOnInstall": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "glassit.alpha": 245
}
